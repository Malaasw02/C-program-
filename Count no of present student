#include <stdio.h>

int main() {
    int n, present = 0, i, status;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for(i = 1; i <= n; i++) {
        printf("Enter 1 if student %d is present: ", i);
        scanf("%d", &status);
        if(status == 1)
            present++;
    }

    printf("Total Present Students = %d\n", present);
    return 0;
}
