#include <stdio.h>

int main() {
    float rain[5], sum = 0, avg;
    int i;

    for(i = 0; i < 5; i++) {
        printf("Enter rainfall of day %d: ", i+1);
        scanf("%f", &rain[i]);
        sum += rain[i];
    }

    avg = sum / 5;
    printf("Average Rainfall = %.2f\n", avg);

    return 0;
}
