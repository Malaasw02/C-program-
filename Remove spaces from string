#include <stdio.h>

int main() {
    char str[50], newstr[50];
    int i, j = 0;

    printf("Enter string: ");
    gets(str);

    for(i = 0; str[i] != '\0'; i++) {
        if(str[i] != ' ')
            newstr[j++] = str[i];
    }
    newstr[j] = '\0';

    printf("Without spaces: %s\n", newstr);
    return 0;
}
