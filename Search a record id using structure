#include <stdio.h>

struct student {
    int id;
    char name[20];
};

int main() {
    struct student s[3];
    int i, search;

    for(i = 0; i < 3; i++)
        scanf("%d %s", &s[i].id, s[i].name);

    printf("Enter ID to search: ");
    scanf("%d", &search);

    for(i = 0; i < 3; i++) {
        if(s[i].id == search) {
            printf("ID:%d Name:%s", s[i].id, s[i].name);
            return 0;
        }
    }

    printf("Record not found");
    return 0;
}
