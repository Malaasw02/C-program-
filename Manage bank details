#include <stdio.h>

struct bank {
    int accno;
    char name[20];
    float balance;
};

int main() {
    struct bank b;

    printf("Enter acc no, name, balance: ");
    scanf("%d %s %f", &b.accno, b.name, &b.balance);

    printf("Account:%d Name:%s Balance:%.2f\n",
           b.accno, b.name, b.balance);
    return 0;
}
