#include <stdio.h>
#include <string.h>

int check(char u[], char p[]) {
    if(strcmp(u, "admin") == 0 && strcmp(p, "1234") == 0)
        return 1;
    return 0;
}

int main() {
    char user[20], pass[20];

    printf("Enter username: ");
    scanf("%s", user);
    printf("Enter password: ");
    scanf("%s", pass);

    if(check(user, pass))
        printf("Login Successful\n");
    else
        printf("Login Failed\n");

    return 0;
}
