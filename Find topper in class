#include <stdio.h>

int main() {
    int marks[5], i, max;

    for(i = 0; i < 5; i++) {
        printf("Enter marks of student %d: ", i+1);
        scanf("%d", &marks[i]);
    }

    max = marks[0];
    for(i = 1; i < 5; i++) {
        if(marks[i] > max)
            max = marks[i];
    }

    printf("Topper Marks = %d\n", max);
    return 0;
}
