#include <stdio.h>
#include <string.h>

int main() {
    char str[20], rev[20];
    int i, j = 0;

    printf("Enter a string: ");
    scanf("%s", str);

    for(i = strlen(str)-1; i >= 0; i--)
        rev[j++] = str[i];
    rev[j] = '\0';

    if(strcmp(str, rev) == 0)
        printf("Palindrome string\n");
    else
        printf("Not a palindrome\n");

    return 0;
}
