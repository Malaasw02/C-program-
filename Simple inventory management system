#include <stdio.h>

struct item {
    int id;
    char name[20];
    int qty;
};

int main() {
    struct item i;

    printf("Enter item id, name, quantity:\n");
    scanf("%d %s %d", &i.id, i.name, &i.qty);

    printf("\nInventory Details\n");
    printf("ID: %d\n", i.id);
    printf("Name: %s\n", i.name);
    printf("Quantity: %d\n", i.qty);

    return 0;
}
